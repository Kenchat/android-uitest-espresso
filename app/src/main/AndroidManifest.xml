<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kbtg.android.espresso">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.kbtg.android.espresso.ui.main.view.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.kbtg.android.espresso.ui.page2.view.Page2Activity" />
        <activity android:name="com.kbtg.android.espresso.ui.nationlist.view.NationListActivity" />
        <activity android:name="com.kbtg.android.espresso.ui.countrydetail.view.NationDetailActivity" />
    </application>

</manifest>